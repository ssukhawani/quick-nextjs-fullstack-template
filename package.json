{
  "name": "quick-nextjs-fullstack-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.x",
    "yarn": "^1.22.19",
    "npm": "please-use-yarn"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css,scss,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "axios": "1.2.6",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.1.6",
    "commitizen": "4.3.0",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3"
  }
}
